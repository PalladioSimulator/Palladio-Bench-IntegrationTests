// calculate an absolute path
global [val projectLocation [concat [get-java-property "user.home"] "\\git\\Palladio-Build-IntegrationTests\\org.palladiosimulator.product.tests.ui\\target\\examples\\PCMFork_Example"]]

// import a project
get-menu -path "File/Import..." | click
with [get-window Import] {
    get-tree | select "General/Existing Projects into Workspace"
    get-button "Next >" | click
}
set-dialog-result Folder $projectLocation
with [get-window Import] {
    get-button "Browse..." | click
    get-button Finish | click
}
