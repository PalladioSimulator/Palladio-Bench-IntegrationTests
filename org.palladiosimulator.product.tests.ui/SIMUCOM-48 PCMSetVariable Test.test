------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

copy-file -source "workspace:/PCMSetVariable_Example/Usage_Scenario_SetVaribleUsage_Response_Time_Tuple_Reference.csv"
-destination [concat [get-java-property "examplesDir" | str | invoke replace "\"" ""] "/" [$projectName]]

with [get-view Experiments] {
    get-tree | select "Local Memory.*"
    get-button "Export Repository to CSV files" | click
}
with [get-window "Select Batch Export Target Folder"] {
    get-group "Location:" | get-button "Workspace..." | click
    with [get-window "Folder Selection"] {
        get-tree | select "PCMSetVariableExample"
        get-button OK | click
    }
    get-button Finish | click
}

with [get-view "Project Explorer" | get-tree] {
    select "PCMSetVariableExample" | get-menu -path Refresh | click
    select "PCMSetVariableExample/SetVariable_Example.*/.*/.*/Usage_Scenario_SetVaribleUsage_Response_Time_Tuple.csv" 
	| get-menu -path Copy | click
    select "PCMSetVariableExample" | get-menu -path Paste | click
}

assert-tables-match [read-csv-file "workspace:/PCMSetVariableExample/Usage_Scenario_SetVaribleUsage_Response_Time_Tuple.csv" | select-columns "Response Time[s]" ] [read-csv-file "workspace:/PCMSetVariableExample/Usage_Scenario_SetVaribleUsage_Response_Time_Tuple_Reference.csv" | select-columns "Response Time[s]"]

delete-file "workspace:/PCMSetVariableExample/Usage_Scenario_SetVaribleUsage_Response_Time_Tuple.csv"
delete-file "workspace:/PCMSetVariableExample/Usage_Scenario_SetVaribleUsage_Response_Time_Tuple_Reference.csv"

get-view "Project Explorer" | get-tree | select "PCMSetVariableExample/SetVariable_Example.*" 
    | get-menu -path Delete | click
get-window "Delete Resources" | get-button OK | click
