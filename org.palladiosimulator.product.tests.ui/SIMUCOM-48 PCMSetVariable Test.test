------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

get-view "Project Explorer" | get-tree | select "PCMSetVariable_Example" | get-menu -path "Run As/Run Configurations..." | click
with [get-window "Run Configurations"] {
    get-tree | select "SimuBench/SetVariable Example"
    get-tab-folder | get-tab-item Simulation | click
    get-group "Simulation Results" | get-group "Data Set" | get-button "Browse..." | click
    with [get-window "Select Datasource..."] {
        get-button "Add.." | click
        get-window "Open EDP2 Data Source" | get-button Finish | click
        get-table | select "LocalMemoryRepository.*"
        get-button OK | click
    }
    get-button Apply | click
    get-button Run | click
}

with [get-view Experiments] {
    get-tree | select "Local Memory.*"
    get-button "Export Repository to CSV files" | click
}
with [get-window "Select Batch Export Target Folder"] {
    get-group "Location:" | get-button "Workspace..." | click
    with [get-window "Folder Selection"] {
        get-tree | select "PCMSetVariable_Example"
        get-button OK | click
    }
    get-button Finish | click
}

with [get-view "Project Explorer" | get-tree] {
    select "PCMSetVariable_Example" | get-menu -path Refresh | click
    select "PCMSetVariable_Example/SetVariable_Example.*/.*/.*/Usage_Scenario_SetVaribleUsage_Response_Time_Tuple.csv" 
    | get-menu -path "Move..." | click
    }
with [get-window "Move Resources"] {
    get-tree | select "PCMSetVariable_Example"
    get-button OK | click
}

assert-tables-match [read-csv-file "workspace:/PCMSetVariable_Example/Usage_Scenario_SetVaribleUsage_Response_Time_Tuple.csv"] [read-csv-file "workspace:/PCMSetVariable_Example/setVariableTestReference.csv"]
------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac--
